        "def sum_nech(l):\n",
        "    x = []\n",
        "    for i in range(len(l)):\n",
        "        if i % 2:\n",
        "            x.append(l[i])\n",
        "    return sum(x)\n",
        "\n",
        "l = [2, 3, 5, 9, 3]\n",
        "sum_nech(l)"
