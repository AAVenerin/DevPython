        "def num_bin(x):\n",
        "    bin_x = str()\n",
        "    while x > 0:\n",
        "        bin_x = str(x % 2) + bin_x\n",
        "        x = x // 2\n",
        "    return int(bin_x)\n",
        "\n",
        "num_bin(45)"
